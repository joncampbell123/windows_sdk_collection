sys                    EQU          $400
immed                  EQU          $200
_SetTrapAddress        OPWORD       $A047

LSETSELECTPATCH	PROC	EXPORT
				IMPORT	(gfpOldPack0):DATA

Selector		EQU		4

				cmpi.w		#$5c, Selector(sp)
				bne.s		NotSetSelect
				move.w		#$a9e7, d0
				movea.l		gfpOldPack0, a0
				_SetTrapAddress ,sys,immed
				movea.l		(sp)+, a0
				lea			$c(sp), sp
				jmp			(a0)
NotSetSelect
				movea.l		gfpOldPack0, a0
				jmp			(a0)
				dc.b	$80, 'LSetSelectPatch', $00
				dc.w	$0000
				ENDP
		END