;-----------------------------------------------------------------------
; ROUTINE:      CreateKey
;
; DESCRIPTION:  Creates a key and fixes the values indicated.
;               Key is Handle\Key1\Key2\...\Keyn. Value list
;               is for Valuen.
;
; INPUTS:       $0: Handle into registry
;               $1: KeyTreeComponents {Key1Info, Key2Info..}
;               $2: ValueList {Value1Info, Value2Info, Value3Info}
;
; OUTPUTS:      $R0: Status: STATUS_SUCCESSFUL
;                            STATUS_FAILED
;                            STATUS_ERROR_CREATEKEY
;                            STATUS_ERROR_CREATEVALUE
;
;               $R1: Handle to created key
;
;------------------------------------------------------------------------

[CreateKey]

    ;
    ; Initialize
    ;

    set Status = STATUS_FAILED
    read-syms RegistryConstants

    ;
    ; traverse down the keytreecomponents and open/create components as
    ; you go along
    ;


    set BaseHandle = $($0)
    set KeyHandle  = $(BaseHandle)

    ForListDo $($1)

        set KeyInfo = $($)
        set KeyName = *($(KeyInfo), 1)

        ;
        ; Try opening the key first

        OpenRegKey $(BaseHandle) "" $(KeyName) $(MaskAllAccess) KeyHandle
        ifint $(RegLastError) != $(!REG_ERROR_SUCCESS)
            ;
            ; Key doesn't exist
            ;
            ; Debug-Output "REGISTRY.INF: Key"$(KeyName)"doesn't exist.  Will create key"

            set RegLastError = 0
            CreateRegKey $(BaseHandle) $(KeyInfo) "" $(MaskAllAccess) "" KeyHandle
            ifint $(RegLastError) != $(!REG_ERROR_SUCCESS)
                set Status = STATUS_ERROR_CREATEKEY
              ;   Debug-Output "REGISTRY.INF: Error in creating key"
                goto endcreate
            endif
        endif

        ifstr(i) $(BaseHandle) != $($0)
            CloseRegKey $(BaseHandle)
            ifint $(RegLastError) != $(!REG_ERROR_SUCCESS)
                set Status = STATUS_ERROR_CREATEKEY
               ;  Debug-Output "REGISTRY.INF: Error in closing base handle"
                goto endcreate
            endif
        endif

        set BaseHandle = $(KeyHandle)

    EndForListDo

    ifstr(i) $($2) != {}
        shell "" AddValueList $(KeyHandle) $($2)
        ifstr(i) $($R0) != STATUS_SUCCESSFUL
            set Status = STATUS_ERROR_CREATEVALUE
            goto endcreate
        endif
    endif
    set Status = STATUS_SUCCESSFUL

endcreate = +
    ifstr(i) $(Status) != STATUS_SUCCESSFUL
        ; Debug-Output "REGISTRY.INF: CreateKey Error:"$(Status)
    endif
    Return $(Status) $(KeyHandle)


;*************************************************************************
;
;     SECTION:   AddValueList
;
;     PURPOSE:   Given a nested list of value items, add each to the given
;                key.   Key is left open.
;
;   ARGUMENTS:   $0    Registry key handle
;                $1    List of value items; for example:
;                          { {ValueName1,0,$(!REG_VT_SZ),$(ValueData1)}, +
;                            {ValueName2,0,$(!REG_VT_SZ),$(ValueData2)} }
;
;     RETURNS:   $R0   Status
;
;
;*************************************************************************

[AddValueList]
   set Status = STATUS_FAILED
   read-syms RegistryConstants

   ForListDo $($1)
       SetRegValue $($0) $($)
       ifint $(RegLastError) != $(!REG_ERROR_SUCCESS)
           ; Debug-Output "REGISTRY.INF: CreateValue failed:"$($)
           goto end_addvaluelist
       endif
   EndForListDo
   set Status = STATUS_SUCCESSFUL

end_addvaluelist = +
   return $(Status)

;**************************************************************************
;
;   SetValue
;
;   Checks if key is present, and if so, sets the value. The key must exist
;
;   $0  Key Handle
;   $1  Key name
;   $2  Value Name
;
;**************************************************************************
[SetValue]
    set Status = STATUS_FAILED
    read-syms RegistryConstants
    
    OpenRegKey $($0) "" $($1) $(!REG_KEY_WRITE) TempKey

    ifint $(RegLastError) != $(!REG_ERROR_SUCCESS)
        ; Debug-Output "REGISTRY.INF: OpenRegKey Failed: "$($)" "$($0)" "$($1)" "$($2)
        goto end_set_value
    endif

    SetRegValue $(TempKey), $($2)
    
    ifint $(RegLastError) != $(!REG_ERROR_SUCCESS)
        ; Debug-Output "REGISTRY.INF: SetRegValue Failed:"$($)
        CloseRegKey $(TempKey)
        goto end_set_value
    endif
    
    set Status = STATUS_SUCCESSFUL

end_set_value = +
    return $(Status) $(KeyValue)
        

;**************************************************************************
;
;   GetValue
;
;   Checks if key is present, and if so, returns the value
;
;   $0  Key Handle
;   $1  Key name
;   $2  Value Name
;
;**************************************************************************
[GetValue]
    set Status = STATUS_FAILED
    read-syms RegistryConstants
    
    OpenRegKey $($0) "" $($1) $(!REG_KEY_READ) TempKey

    ifint $(RegLastError) != $(!REG_ERROR_SUCCESS)
        ; Debug-Output "REGISTRY.INF: OpenRegKey Failed: "$($)" "$($0)" "$($1)" "$($2)
        goto end_get_value
    endif

    GetRegValue $(TempKey), $($2), KeyValue
    
    ifint $(RegLastError) != $(!REG_ERROR_SUCCESS)
        ; Debug-Output "REGISTRY.INF: GetRegValue Failed:"$($)
        CloseRegKey $(TempKey)
        goto end_get_value
    endif
    
    set Status = STATUS_SUCCESSFUL

end_get_value = +
    return $(Status) $(KeyValue)
        

;**************************************************************************
; SHELL COMMANDS SCRIPT
;**************************************************************************

[Shell Commands]

    ;
    ; Initialise the app, by setting the caption and loading the support library
    ;
    set-title "Win32 SDK"
    set Exit_Code = $(!SETUP_ERROR_GENERAL)
    install   LoadSetupLibrary

    StartWait

    ;
    ; read general constants
    ;

    set-subst LF = "\n"
    read-syms GeneralConstants


    ;
    ; read and detect the processor we are working on
    ;

    read-syms ProcessorID
    read-syms PlatformID
    read-syms ProcessorVar
    detect    ProcessorVar

    ;
    ; Check processor type, see if it is supported, if so what platform does it
    ; belong to.
    ;

    ifstr(i) $(STF_PROCESSOR) == $(ProcessorID_I386)
        set STF_PLATFORM = $(PlatformID_I386)
    else-ifstr(i) $(STF_PROCESSOR) == $(ProcessorID_I486)
        set STF_PLATFORM = $(PlatformID_I386)
    else-ifstr(i) $(STF_PROCESSOR) == $(ProcessorID_I586)
        set STF_PLATFORM = $(PlatformID_I386)
    else-ifstr(i) $(STF_PROCESSOR) == $(ProcessorID_R4000)
        set STF_PLATFORM = $(PlatformID_MIPS)
    else-ifstr(i) $(STF_PROCESSOR) == $(ProcessorID_ALPHA)
        set STF_PLATFORM = $(PlatformID_ALPHA)
    else-ifstr(i) $(STF_PROCESSOR) == $(ProcessorID_PPC601)
        set STF_PLATFORM = $(PlatformID_PPC)
    else-ifstr(i) $(STF_PROCESSOR) == $(ProcessorID_PPC603)
        set STF_PLATFORM = $(PlatformID_PPC)
    else-ifstr(i) $(STF_PROCESSOR) == $(ProcessorID_PPC604)
        set STF_PLATFORM = $(PlatformID_PPC)
    else-ifstr(i) $(STF_PROCESSOR) == $(ProcessorID_PPC620)
        set STF_PLATFORM = $(PlatformID_PPC)
    else

        ; Unfortunately, we haven't detected language yet,
        ;  so default to English, report error, and quit.

        STF_LANGUAGE = "ENG"
        shell "subroutn.inf" SetupMessage $(!STF_LANGUAGE) "FATAL" $(String1)
        goto setupnotdone
    endif


    ;
    ; Call into our DLL to determine Locale
    ;  Although NEC is not a proper Windows locale,
    ;  it is a possible return value from our GetWindowsLocaleID()
    ;

    install   LoadSDKSetupLibrary

    read-syms LocaleVar
    detect    LocaleVar
    ifstr(i) $(STF_LOCALE) == ""
        set STF_LOCALE = "usa"
    endif

    ; Debug-Output $(STF_LOCALE)


    ;
    ; Figure out STF_LANGUAGE language ourselves based on STF_LOCALE
    ;  Notice that GetLanguage() in setupdll.dll is effectively a no-op.
    ;
    ; Notice that we overload STF_LANGUAGE to represent the language that
    ;  the UI is displayed in, and to conditionally install certain files.
    ;
    set STF_REGION     = "Default"
    set STF_UI_LANGUAGE = $(LANGID_ENG)

    ifstr(i) $(STF_LOCALE) == "japan"
        set STF_LANGUAGE = $(LANGID_JPN)
        set STF_REGION     = $(REGION_FE)
        set STF_UI_LANGUAGE = $(LANGID_JPN)
    else-ifstr(i) $(STF_LOCALE) == "nec"
        set STF_LANGUAGE = $(LANGID_JPN)
        set STF_REGION     = $(REGION_FE)
        set STF_UI_LANGUAGE = $(LANGID_JPN)
    else-ifstr(i) $(STF_LOCALE) == "taiwan"
        set STF_LANGUAGE = $(LANGID_CHT)
        set STF_REGION     = $(REGION_FE)
    else-ifstr(i) $(STF_LOCALE) == "korea"
        set STF_LANGUAGE = $(LANGID_KOR)
        set STF_REGION     = $(REGION_FE)
    else-ifstr(i) $(STF_LOCALE) == "prc"
        set STF_LANGUAGE = $(LANGID_CHS)
        set STF_REGION     = $(REGION_FE)
    else
        set STF_LANGUAGE = $(LANGID_ENG)
    endif

    ;MCTESTING
    ;************************************
    ;set STF_LANGUAGE = $(LANGID_JPN)
    ;set STF_REGION     = $(REGION_FE)
    ;set STF_UI_LANGUAGE = $(LANGID_JPN)
    ;************************************
    ;end MCTESTING


    ; Debug-Output $(STF_LANGUAGE)

    ; Detect OS version, and make install decisions based on that
    LibraryProcedure  OSVersion, $(SDKLIBHANDLE), SetupGetOSVersion

    read-syms Strings$(!STF_UI_LANGUAGE)
    read-syms Billboards$(!STF_UI_LANGUAGE)

    read-syms  RegistryConstants

    ;
    ; Call the library function to see if we have admin privileges
    ;

    LibraryProcedure STATUS, $(!LIBHANDLE), TestAdmin
    ifstr(i) $(STATUS) == "YES"
 	set PRIVILEGES = SYSTEM
    else
        set PRIVILEGES = USER
    endif

    ;
    ; read the UI Variables and detect their setting on the current machine
    ;

    read-syms UiVars
    detect    UiVars

    read-syms Build-Number
    detect    Build-Number

    set VolumeFreeList = {}
    ;
    ; Call our DLL to get the real disk size (to work around 4Gb issue)
    ;
    ForListDo $(VolumeList)
        ;Debug-Output $($)
        LibraryProcedure RetSize, $(SDKLIBHANDLE), SetupGetDiskFreeSpace $($)
        ;Debug-Output $(RetSize)
        set VolumeFreeList = >($(VolumeFreeList), $(RetSize))
    EndForListDo

    read-syms Vars$(STF_PLATFORM)
    read-syms VarsPlatformCommon
    read-syms VarsOS

    ;
    ; Dynamically adjust the variables just read.
    ;

    ifstr(i) $(STF_HOSTOS) == "WIN95"
        ; Win95 debug/nodebug are bigger than NT dbg files.
        set-add MaxToolsSize = $(MaxToolsSize) 9
    endif

    EndWait


welcome =+
    ifstr(i) "351" == *($(OSVersion), 2)*($(OSVersion), 3)
        read-syms Welcome351Dlg$(STF_UI_LANGUAGE)
    else
        read-syms WelcomeDlg$(STF_UI_LANGUAGE)
    endif
    ui start "Welcome" $(SDKLIBHANDLE)
    ifstr(i) $(DLGEVENT) == "CONTINUE"
        ui pop 1
        goto queryinstallmode
    else-ifstr(i) $(DLGEVENT) == "EXIT"
        shell "" QueryUserQuit
        ifstr(i) $($R1) == "OK"
            ui pop 1
            goto setupnotdone
        else
            goto welcome
        endif
    else
        goto end
    endif

    ;
    ; show install mode dialog and query if we are to operate in express mode
    ; or in full install
    ;

queryinstallmode =+
    ifstr(i) $(AcceptLicense) != "TRUE"
        ;
        ; Show license agreement and get acceptance
        ;
        ;Debug-Output "Calling ShowLicenseAgreement"
        ifstr(i) $(STF_UI_LANGUAGE) == $(LANGID_JPN) 
    	    ; Note License.txt and the JapanLicense need to be localized
	        read-syms JapanLicense
            LibraryProcedure RetCode, $(SDKLIBHANDLE), ShowLicenseAgreement $(STF_SRCDIR)license\license.txt $(LICENSEJ1) $(LICENSEJ2) $(LICENSEJ3) $(LICENSEJ4) $(LICENSEJ5) $(LICENSEJ6)  $( LICENSEJ7)
        else
    	    LibraryProcedure RetCode, $(SDKLIBHANDLE), ShowLicenseAgreement $(STF_SRCDIR)license\license.txt
        endif
    
        ifstr(i) $(RetCode) != "SUCCESS"
	        goto setupnotdone
        endif

        set AcceptLicense = "TRUE"
    endif

    read-syms InstallModeDlg$(STF_UI_LANGUAGE)
    ui start "InstallMode" $(SDKLIBHANDLE)
    ifstr(i) $(DLGEVENT) == "CONTINUE"
        ui pop 1
        set STF_INSTALL_MODE = $(ButtonChecked)
        goto setdestination
    else-ifstr(i) $(DLGEVENT) == "EXIT"
        shell "" QueryUserQuit
        ifstr(i) $($R1) == "OK"
            ui pop 1
            goto setupnotdone
        else
            goto queryinstallmode
        endif
    else-ifstr(i) $(DLGEVENT) == "BACK"
        ui pop 1
        goto welcome
    else
        goto end
    endif

setdestination =+

    ;
    ; go through the volume list and form the destination volume list of
    ; all volumes which have the size needed to install the mstools.
    ;
    set DestVolumeList = {}
    set DestVolumeFreeList = {}

    ForListDo $(VolumeList)

       set CurrentVolume      = *($(VolumeList), $(#))
       set CurrentVolumeFree  = *($(VolumeFreeList), $(#))
       set CurrentVolumeFS    = *($(VolumeFSList), $(#))

       set DestVolumeList     = >($(DestVolumeList),     $(CurrentVolume))
       set DestVolumeFreeList = >($(DestVolumeFreeList), $(CurrentVolumeFree))

skipvol=+
    EndForListDo

    ;
    ; destination drive is the drive with the maximum space
    ;

    set DestVolume     = ""
    set DestVolumeFree = 0

    ForListDo $(DestVolumeList)
        set Volume     = $($)
        set VolumeFree = *($(DestVolumeFreeList), ~($(DestVolumeList), $(Volume)))

        ifint $(VolumeFree) > $(DestVolumeFree)
            set DestVolume     = $(Volume)
            set DestVolumeFree = $(VolumeFree)
        endif

    EndForListDo

    ;
    ; Check for previous installs, and set default target install
    ;

    set KeySDKPath = "SOFTWARE\Microsoft\Win32SDK\Directories"

    shell "" GetValue $(!REG_H_CUSER) $(KeySDKPath) "Install Dir"

    ifstr(i) $($R0) == STATUS_FAILED
        set MstoolsDir = $(DestVolume)$(MSTOOLS)
    else
        set MstoolsDir = *($($R1),4)
        GetDriveInPath DestVolume $(MstoolsDir)
        set DestVolumeFree = *($(DestVolumeFreeList), ~($(DestVolumeList), $(DestVolume)))
    endif


    ;
    ; display tools destination, size available and needed for
    ; user approval
    ;

destination =+

    ifstr(i) $(STF_INSTALL_MODE) == $(MinInteract)
       ifint $(DestVolumeFree) < $(MaxToolsSize)
           shell "subroutn.inf" SetupMessage $(!STF_UI_LANGUAGE) "FATAL" $(String8)$(MaxToolsSize)$(String10)$(String12)
           set STF_INSTALL_MODE = $(MaxInteract)
           goto queryinstallmode
       else
           goto mstooldirs
       endif
    endif

    read-syms DestinationDlg$(STF_UI_LANGUAGE)
    ;
    ; Fix for Append backslash problem--affecting MSTOOLS environment var
    ;
    ifstr(i)  $(MstoolsRoot) != ""
             set MstoolsDir = $(MstoolsRoot)
             set EditTextIn = $(MstoolsRoot)
    endif
    ui start "Destination" $(SDKLIBHANDLE)
    ifstr(i) $(DLGEVENT) == "NOTIFY"

        ifstr(i) $(Combo1Out) != $(DestVolume)
            set ReInit = YES
            set DestVolume     = $(Combo1Out)
            set DestVolumeFree = *($(DestVolumeFreeList), ~($(DestVolumeList), $(DestVolume)))
            set MstoolsDir     = $(DestVolume)$(MSTOOLS)
        else
            set ReInit = NO
        endif
        goto destination

    else-ifstr(i) $(DLGEVENT) == "CONTINUE"
        set ReInit = NO
        LibraryProcedure IsFullPath, $(SDKLIBHANDLE), SDKCheckPathFullPathSpec $(EditTextOut)
        ifstr(i) $(IsFullPath) == "NO"
            shell "subroutn.inf" SetupMessage $(!STF_UI_LANGUAGE) "NONFATAL" $(String13)$(DestVolume)$(MSTOOLS)
            goto destination
        else
            ;
            ; see if there is any whitespace in the filename, if so, bail
            ;
            LibraryProcedure STATUS, $(!LIBHANDLE), WhiteSpaceCheck $(EditTextOut)
            ifstr(i) $(STATUS) == "YES"
                shell "subroutn.inf" SetupMessage $(!STF_UI_LANGUAGE) "NONFATAL" $(String20)
                goto destination
            else
                GetDriveInPath Drive, $(EditTextOut)
                Ifcontains(i) $(Drive) in $(DestVolumeList)
                    set DestVolume     = $(Drive)
                    set DestVolumeFree = *($(DestVolumeFreeList), ~($(DestVolumeList), $(DestVolume)))
                    set MstoolsDir     = $(EditTextOut)
                else
                    set VolumeFreeNetwork = 0
                    ;
                    ; Call our DLL to get the real disk size (to work around 4Gb issue)
                    ;
                    ;Debug-Output $(EditTextOut)
                    LibraryProcedure RetSize, $(SDKLIBHANDLE), SetupGetDiskFreeSpace $(EditTextOut)
                    ;Debug-Output $(RetSize)
                    set VolumeFreeNetwork = $(RetSize)

                    shell "subroutn.inf" SetupMessage $(!STF_UI_LANGUAGE) "WARNING" $(String16)
                    ifstr(i) $($R0) != STATUS_SUCCESSFUL
			            goto destination
                    else
                        ifstr(i) $($R1) == "OK"
			                set DestVolume = $(Drive)
			                set DestVolumeFree = $(VolumeFreeNetwork)
			                set MstoolsDir = $(EditTextOut)
                        else
                            set ReInit = NO
                            goto destination
                        endif
                    endif
                endif
            endif
        endif

        ui pop 1
        goto mstooldirs

    else-ifstr(i) $(DLGEVENT) == "EXIT"

        set ReInit = NO
        shell "" QueryUserQuit
        ifstr(i) $($R1) == "OK"
            ui pop 1
            goto setupnotdone
        else
            goto destination
        endif
    else-ifstr(i) $(DLGEVENT) == "BACK"
        ui pop 1
        goto queryinstallmode
    else
        goto end
    endif


mstooldirs = +

    set MstoolsRoot = $(MstoolsDir)
    LibraryProcedure MstoolsDir, $(LIBHANDLE), AppendBackSlash $(MstoolsDir)
    ;
    ; First find all the destination directories for the tools
    ;

    set MstoolsPath    = $(MstoolsDir)bin
    set MstoolsLib     = $(MstoolsDir)lib
    set MstoolsInclude = $(MstoolsDir)include
    set MstoolsInit    = $(MstoolsDir)init
    set MstoolsHelp    = $(MstoolsDir)Help

    ;
    ; mstool install.  ask user for options on mstool install.
    ;

mstools = +
    ifstr(i) $(STF_INSTALL_MODE) == $(MinInteract)
        goto mstoolsinstall
    endif

    ifint $(DestVolumeFree) < $(MaxToolsSize)
        set DoTools          = $(NotChosen)
        set DoOnlineHelp     = $(NotChosen)
        set DoMSSetup        = $(NotChosen)
        set DoPosix          = $(NotChosen)
        set DoSamples        = $(NotChosen)
        set DoDebugDLL       = $(NotChosen)
    else
        set DoTools          = $(Chosen)
        set DoOnlineHelp     = $(Chosen)
        set DoMSSetup        = $(Chosen)
        set DoPosix          = $(Chosen)
        set DoSamples        = $(Chosen)
        set DoDebugDLL       = $(Chosen)
    endif

mstoolsrepeat = +
    set FreeForOptional = $(DestVolumeFree)
    ifstr(i) $(FreeForOptional) == 0
        shell "subroutn.inf" SetupMessage $(!STF_UI_LANGUAGE) "NONFATAL" $(String15)
    endif

    set SamplesSize = 0
    set ToolsSize   = 0

    ifstr(i) $(STF_HOSTOS) == "WINNT"
        set DebugDLLSize = $(WinntDLLSize)
    else
        set DebugDLLSize = $(Win95DLLSize)
    endif

	ForListDo $(ToolsList)
	    ifstr(i) $(Do$($)) == $(Chosen)
	    	set-add ToolsSize = $(ToolsSize) $($($)Size)
	    endif
	EndForListDo

	ForListDo $(SamplesList)
	    ifstr(i) $(Do$($)) == $(Chosen)
	    	set-add SamplesSize = $(SamplesSize) $($($)Size)
	    endif
	EndForListDo

    read-syms MSToolsOptionsDlg$(STF_UI_LANGUAGE)
    ui start "Options" $(SDKLIBHANDLE)
    ifstr(i) $(DLGEVENT) == "CONTINUE"
        set DoTools          = *($(CheckItemsOut), 1)
        set DoOnlineHelp     = *($(CheckItemsOut), 2)
        set DoSamples        = *($(CheckItemsOut), 3)
        set DoDebugDLL       = *($(CheckItemsOut), 4)
        set DoMSSetup        = *($(CheckItemsOut), 5)
        set DoPosix          = *($(CheckItemsOut), 6)
        ui pop 1
        ifstr(i) $(DoWin32) == $(NotChosen)
            set DoWin32NT = $(NotChosen)
            set DoWin3295 = $(NotChosen)
        endif
        ifstr(i) $(DoSDKTools) == $(NotChosen)
            set DoSDKToolsNT = $(NotChosen)
            set DoSDKTools95 = $(NotChosen)
        endif
        ifstr(i) $(DoTools) == $(NotChosen)
            ForListDo $(ToolsList)
                set Do$($) = $(NotChosen)
            EndForListDo
        endif
        goto checkdestfree

    else-ifstr(i) $(DLGEVENT) == "BACK"
        ui pop 1
        goto destination

    else-ifstr(i) $(DLGEVENT) == "EXIT"
        shell "" QueryUserQuit
        ifstr(i) $($R1) == "OK"
            ui pop 1
            goto setupnotdone
        else
            goto mstoolsrepeat
        endif
	else-ifstr(i) $(DLGEVENT) == "CUSTOMISE"
        set DoTools          = *($(CheckItemsOut), 1)
        set DoOnlineHelp     = *($(CheckItemsOut), 2)
        set DoSamples        = *($(CheckItemsOut), 3)
        set DoDebugDLL       = *($(CheckItemsOut), 4)
        set DoMSSetup        = *($(CheckItemsOut), 5)
        set DoPosix          = *($(CheckItemsOut), 6)
	    ui pop 1

        ifstr(i) $(ButtonChecked) == "1"
            read-syms THLDlg$(!STF_UI_LANGUAGE)
	        ui start "Options" $(!SDKLIBHANDLE)
	        ifstr(i) $(DLGEVENT) == "DLGBUTTON0" ;button titled OK, IDC_BTN0
	            set DoBins	    = *($(CheckItemsOut), 1)
                set DoHeaders   = *($(CheckItemsOut), 2)
	            set DoLibs	    = *($(CheckItemsOut), 3)
	            set DoBinHelp	= *($(CheckItemsOut), 4)
            endif

            ui pop 1
            goto mstoolsrepeat
        else
	        read-syms SDKSamplesDlg$(STF_UI_LANGUAGE)
	        ui start "Options" $(SDKLIBHANDLE)
	        ifstr(i) $(DLGEVENT) == "DLGBUTTON0" ;button titled OK, IDC_BTN0
		        set DoWin32	    = *($(CheckItemsOut), 1)
                set DoFrmwork   = *($(CheckItemsOut), 2)
		        set DoOLE	    = *($(CheckItemsOut), 3)
		        set DoOpenGL	= *($(CheckItemsOut), 4)
		        set DoRPC	    = *($(CheckItemsOut), 5)
		        set DoSDKTools	= *($(CheckItemsOut), 6)
		        set DoMM	    = *($(CheckItemsOut), 7)
		        set DoMapiSam   = *($(CheckItemsOut), 8)
		        set Do16TapiPen	    = *($(CheckItemsOut), 9)
            endif
        
	        ui pop 1
	        goto mstoolsrepeat

        endif
    else
        goto "end"
    endif

checkdestfree =+
    set ToolsSize = 0
    set SamplesSize = 0
    set TotalRequiredSize = 0
    set BytesFree = $(DestVolumeFree)

	ForListDo $(ToolsList)
	    ifstr(i) $(Do$($)) == $(Chosen)
	    	set-add ToolsSize = $(ToolsSize) $($($)Size)
	    endif
	EndForListDo

	ForListDo $(SamplesList)
	    ifstr(i) $(Do$($)) == $(Chosen)
	    	set-add SamplesSize = $(SamplesSize) $($($)Size)
	    endif
	EndForListDo

    ForListDo $(TotalList)
        ifstr(i) $(Do$($)) == $(Chosen)
            set-add TotalRequiredSize = $(TotalRequiredSize) $($($)Size)
        endif
    EndForListDo

    ;
    ; TotalRequiredSize is bytes, but BytesFree is MB... Convert bytes to MB
    ;  because other direction causes overflow on drives with > 2 GB free.
    ;

    set-div TotalRequiredSize = $(TotalRequiredSize) 1024
    set-div TotalRequiredSize = $(TotalRequiredSize) 1024


;  this to allows forced install even if not enough drive space
;  this can happen when reinstalling the SDK where most files are just replace with
;  the same size files or possibly when using disk compression
    ifint $(TotalRequiredSize) > $(BytesFree)

                    shell "subroutn.inf" SetupMessage $(!STF_UI_LANGUAGE) "WARNING" $(String17)
                    ifstr(i) $($R0) != STATUS_SUCCESSFUL
			            goto mstoolsrepeat
                    else
                        ifstr(i) $($R1) == "OK"
                        ;do nothing and continue
	                else
                            set ReInit = NO
                            goto mstoolsrepeat
                        endif
                   endif
    endif


    ;
    ; build mstools copy list
    ;

mstoolsinstall	=+
    
    StartWait
    shell "subroutn.inf" PushBillboard STATUSDLG $(Billboard1)
    install Install-CopyToolsFiles
    shell "subroutn.inf" PopBillboard
    EndWait

finish = +

    ;
    ; first copy all the files over
    ;

    ifstr(i) $(DoBins) == $(Chosen)

        ;
        ; Execute the self-extracting EXEs
        ;

        ifstr(i) "351" == *($(OSVersion), 2)*($(OSVersion), 3)
            ; don't install them
        else
            RunProgram ExitCode "" "" $(STF_SRCDIR)mstools\bin\$(STF_PLATFORM)\dflaydll.exe
            RunProgram ExitCode "" "" $(STF_SRCDIR)mstools\bin\$(STF_PLATFORM)\ccdist.exe -q
        endif
    endif

    read-syms ProgressCopy$(STF_UI_LANGUAGE)
    install Install-All-Files-Now
    ifstr(i) $(STF_INSTALL_OUTCOME) == "STF_SUCCESS"
        goto dodbg
    else
        shell "subroutn.inf" SetupMessage $(!STF_UI_LANGUAGE) "FATAL" $(String4)
        goto setupnotdone
    endif

dodbg = +
    
    ifstr(i) $(DoDebugDLL) == $(Chosen)
        ;
        ; Note win95 dbg files are copied at the bottom of this script
        ; in section "6. Debug symbols for Window 95 - optional"
        ; 
        ifstr(i) $(STF_HOSTOS) == "WINNT"

          ;if build matches files should already be in the drive
          ifstr(i) $(STF_LANGUAGE) == "don't install" 

            ;check for identifier file
            set FileToCheck = $(STF_SRCDIR)"support\debug\"$(STF_PLATFORM)"\symbols\dll\advapi32.dbg"
            install Install-SeeIfFileIsPresent

            ;if not found, check for identifier file compressed
            ifstr(i) $(STATUS) != "YES"
               set FileToCheck = $(STF_SRCDIR)"support\debug\"$(STF_PLATFORM)"\symbols\dll\advapi32.db_"
               install Install-SeeIfFileIsPresent
            endif

            ifstr(i) $(STATUS) != "YES"
                ; neither compressed or uncompress identifier file found
                ; so let's tell the used we did not find the CD
        	shell "subroutn.inf" SetupMessage $(!STF_UI_LANGUAGE) "WARNING" $(String18)

	        ifstr(i) $($R0) != STATUS_SUCCESSFUL
	                goto dbgfilecontinue
                else

	                ifstr(i) $($R1) == "OK"
                                ; user hit okay to tell us to look for the CD again
		                goto dodbg
	                else
                                ; user gave up
 		                shell "subroutn.inf" SetupMessage $(!STF_UI_LANGUAGE) "NONFATAL" $(String21)
		                goto mstoolsconfig
	                endif ;ok
                endif ; user warning

            else
                ; found the id file, do the file copy
                goto dbgfilecontinue

            endif ; file present

      ;
      ;If not right build number, prompt for disk, this helps Japan, etc
      ;
      else

            shell "subroutn.inf" DoAskSourceEx $(STF_SRCDIR) $(String19)

            ifint $($ShellCode) != $(!SHELL_CODE_OK)
                ;Debug-Output "shelling DoAskSource failed"
                shell "subroutn.inf" SetupMessage $(!STF_UI_LANGUAGE) "NONFATAL" $(String21)
                goto mstoolsconfig
            endif

            ifstr(i) $($R0) == STATUS_SUCCESSFUL
                set STF_SRCDIR = $($R1)
                ifstr(i) $($R2) != ""
                    set DrivesToFree = >($(DrivesToFree), $($R2))
                endif
            else
                shell "subroutn.inf" SetupMessage $(!STF_UI_LANGUAGE) "NONFATAL" $(String21)
                goto mstoolsconfig
            endif

            ;check for identifier file
            set FileToCheck = $(STF_SRCDIR)"support\debug\"$(STF_PLATFORM)"\symbols\dll\advapi32.dbg"
            install Install-SeeIfFileIsPresent

            ;if not found, check for identifier file compressed
            ifstr(i) $(STATUS) != "YES"
               set FileToCheck = $(STF_SRCDIR)"support\debug\"$(STF_PLATFORM)"\symbols\dll\advapi32.db_"
               install Install-SeeIfFileIsPresent
            endif

            ifstr(i) $(STATUS) != "YES"
                ; neither compressed or uncompress identifier file found
                ; so let's tell the used we did not find the CD
        	shell "subroutn.inf" SetupMessage $(!STF_UI_LANGUAGE) "WARNING" $(String18)

	        ifstr(i) $($R0) != STATUS_SUCCESSFUL
	                goto dbgfilecontinue
                else

	                ifstr(i) $($R1) == "OK"
                                ; user hit okay to tell us to look for the CD again
		                goto dodbg
	                else
                                ; user gave up
 		                shell "subroutn.inf" SetupMessage $(!STF_UI_LANGUAGE) "NONFATAL" $(String21)
		                goto mstoolsconfig
	                endif ;ok
                endif ; user warning

            else
                ; found the id file, do the file copy
                goto dbgfilecontinue

            endif ; file present
        
          endif ; build number

       endif ; winnt

       goto mstoolsconfig
        
dbgfilecontinue =+

            install Install-DBGFiles

            install Install-All-Files-Now
            ifstr(i) $(STF_INSTALL_OUTCOME) == "STF_SUCCESS"
                goto mstoolsconfig
            else
                shell "subroutn.inf" SetupMessage $(!STF_UI_LANGUAGE) "NONFATAL" $(String21)
            endif
        endif
    endif

mstoolsconfig=+
    ifstr(i) $(STF_HOSTOS) == "WINNT"
    read-syms EnvironmentVars
    detect    EnvironmentVars

    ;
    ; first initialize the variables which indicate whether new values are
    ; to be set for the variables or not
    ;
    ForListDo {Path, Lib, Include, Init}
        set FSet$($)Var = FALSE
    EndForListDo

    ;
    ; For the Path Lib and Include variables see if we can prepend them
    ; onto the current variables
    ;

    set dolist = {Bins, Libs, Headers}
    ForListDo {Path, Lib, Include}
        ifstr(i) $(Do*($(dolist),$(#))) == $(Chosen)
            set Var = $($)
            ifstr(i) $($(Var)EnvVar) == {}
                set $(Var)EnvVar = {$(Var), 0, $(!REG_VT_EXPAND_SZ), {$(Mstools$(Var))}}
                set FSet$(Var)Var = TRUE
            else
                set VarTitle   = *($($(Var)EnvVar), 2)
                set VarRegType = *($($(Var)EnvVar), 3)
                set VarValue   = *($($(Var)EnvVar), 4)
	            ifstr(i) $(Var) == "Path"
	    	        ifstr(i) $(PRIVILEGES) == "SYSTEM"
            	        shell "" AppendNewValueToEnvVar $(VarValue) $(Mstools$(Var))
	    	        else
		                shell "" PrependNewValueToEnvVar $(VarValue) $(Mstools$(Var))
	    	        endif
	            else
                    shell "" PrependNewValueToEnvVar $(VarValue) $(Mstools$(Var))
	            endif
    
                ifstr(i) $($R0) == STATUS_FAILED
                    shell "subroutn.inf" SetupMessage $(!STF_UI_LANGUAGE) "FATAL" $(String6)
                    goto setupnotdone
                endif
    
                ifstr(i) $($R1) == "TRUE"
                    set FSet$(Var)Var = TRUE
                    set $(Var)EnvVar = {$(Var), $(VarTitle), $(VarRegType), $($R2)}
                endif
            endif
        endif
    EndForListDo

    ;
    ; If a user, then add %EnvVar% so we don't over-write...
    ;

    ifstr(i) $(PRIVILEGES) == "USER"
        set dolist = {Headers,Libs}
        ifstr(i) $(Do*($(dolist),$(#))) == $(Chosen)
            ForListDo {Include, Lib}
                set Var = $($)
                ifstr(i) $($(Var)EnvVar) == {}
                    set $(Var)EnvVar = {$(Var), 0, $(!REG_VT_EXPAND_SZ), {%$(Var)%}}
                    set FSet$(Var)Var = TRUE
                else
                    set VarTitle   = *($($(Var)EnvVar), 2)
                    set VarRegType = *($($(Var)EnvVar), 3)
                    set VarValue   = *($($(Var)EnvVar), 4)
    
                    shell "" PrependNewValueToEnvVar $(VarValue) %$(Var)%
    
    
                    ifstr(i) $($R0) == STATUS_FAILED
                        shell "subroutn.inf" SetupMessage $(!STF_UI_LANGUAGE) "FATAL" $(String6)
                        goto setupnotdone
                    endif
    
                    ifstr(i) $($R1) == "TRUE"
                        set FSet$(Var)Var = TRUE
                        set $(Var)EnvVar = {$(Var), $(VarTitle), $(VarRegType), $($R2)}
                    endif
                endif
            EndForListDo
        endif
    endif

    ;
    ; make cpu variable - STF_PLATFORM
    ;
    set FSetCpuVar = TRUE
    ifstr(i) $(!STF_PLATFORM) == $(!PlatformID_I386)
        set CpuEnvVar = {Cpu, 0, $(!REG_VT_SZ), i386}
    else-ifstr(i) $(!STF_PLATFORM) == $(!PlatformID_ALPHA)
	set CpuEnvVar = {Cpu, 0, $(!REG_VT_SZ), ALPHA}
    else-ifstr(i) $(!STF_PLATFORM) == $(!PlatformID_PPC)
	set CpuEnvVar = {Cpu, 0, $(!REG_VT_SZ), PPC}
    else
        set CpuEnvVar = {Cpu, 0, $(!REG_VT_SZ), MIPS}
    endif

    ;
    ; make Mstools Root variable - MstoolsDir
    ;
    set FSetMstoolsRootVar = TRUE
    set MstoolsRootEnvVar = {Mstools, 0, $(!REG_VT_SZ), $(MstoolsRoot)}

    ;
    ; set TARGETLANG if Japanese system
    ;

    ifstr(i) $(STF_LANGUAGE) == $(LANGID_JPN)
      set FSetTARGETLANGVar = TRUE
      set TARGETLANG = LANG_JAPANESE
    else-ifstr(i) $(STF_LANGUAGE) == $(LANGID_CHT)
      set FSetTARGETLANGVar = TRUE
      set TARGETLANG = LANG_CHINESE
    else-ifstr(i) $(STF_LANGUAGE) == $(LANGID_KOR)
      set FSetTARGETLANGVar = TRUE
      set TARGETLANG = LANG_KOREAN
    else-ifstr(i) $(STF_LANGUAGE) == $(LANGID_CHS)
      set FSetTARGETLANGVar = TRUE
      set TARGETLANG = LANG_CHINESE
    endif


    ;
    ; modify all the environment variables
    ;

    ForListDo {Lib, Include, Cpu, Path, MstoolsRoot, TARGETLANG}
        ;
        ; if the variable to set the variable is set

        ifstr(i) $(FSet$($)Var) == "TRUE"
            shell "" SetEnvironmentString $($($)EnvVar) $(PRIVILEGES)
            ifstr(i) $($R0) == STATUS_FAILED
                shell "subroutn.inf" SetupMessage $(!STF_UI_LANGUAGE) "FATAL" $(String7)
            endif
        endif
    EndForListDo

    endif

    ;
    ; Check to see if VC++ is installed
    ;  First VC4, if not that, then VC2
    ;

    set KeyPath = "SOFTWARE\Microsoft\Developer\Directories"
    shell "" GetValue $(!REG_H_CUSER) $(KeyPath) "Install Dirs"

    ifstr(i) $($R0) == STATUS_FAILED
        set KeyPath = "SOFTWARE\Microsoft\Visual C++ 2.0\Directories"
        shell "" GetValue $(!REG_H_CUSER) $(KeyPath) "Install Dirs"
    endif

    ifstr(i) $($R0) == STATUS_FAILED
        set VCPresent = "No"
    else
        set VCPresent = "Yes"
        set VCRoot = *($($R1),4)
    endif

    ifstr(i) $(VCPresent) == "Yes"
        ifstr(i) $(STF_PLATFORM) == $(PlatformID_I386)
            set vcplat = "x86"
        endif
        ifstr(i) $(STF_PLATFORM) == $(PlatformID_MIPS)
            set vcplat = "MIPS"
        endif
        ifstr(i) $(STF_PLATFORM) == $(PlatformID_ALPHA)
            set vcplat = "ALPHA"
        endif
        ifstr(i) $(STF_PLATFORM) == $(PlatformID_PPC)
            set vcplat = "PPC"
        endif
    
        set dolist = {Headers, Libs, Bins}
        set vclist = {Include, Library, Path}
        ForListDo {Include, Lib, Path}
            ifstr(i) $(Do*($(dolist),$(#))) == $(Chosen)
                set VCDirs = *($(vclist),$(#))
                set KeyPath = "SOFTWARE\Microsoft\Visual C++ 2.0\Platforms\Win32 ("$(vcplat)")"
                shell "" GetValue $(!REG_H_CUSER) $(KeyPath) $(VCDirs)" Dirs"
                ifstr(i) $($R0) != STATUS_FAILED
                    set ExistingValue = *($($R1),4)
                    LibraryProcedure STATUS, $(!SDKLIBHANDLE), CheckStrIStr $(Mstools$($)) $(ExistingValue)

                    ;
                    ; if StrIStr failed, then the string isn't present, and we
                    ; can add it
                    ;

                    ifstr(i) $(STATUS) == "FAILED" 

                        set NewValue = $(Mstools$($))";"$(ExistingValue)
                        set KeyValue = { $(VCDirs)" Dirs", $(NoTitle), $(!REG_VT_SZ), $(NewValue) }
                        shell "" SetValue $(!REG_H_CUSER) $(KeyPath) $(KeyValue)
                    endif
    
                endif 
            endif
        EndForListDo
    endif


    install Install-MSTools-Groups

    ifstr(i) $(DoBins) == $(Chosen)
        ;
        ; modify the registry to use windbg instead of drwatson and clear auto
        ;
    
        set KeyPath = { +
                       {SOFTWARE,		$(NoTitle),	$(MaskAllAccess)}, +
                       {Microsoft,      $(NoTitle),	$(MaskAllAccess)}, +
                       {"Windows NT",	$(NoTitle),	$(MaskAllAccess)}, +
		               {CurrentVersion,	$(NoTitle),	$(MaskAllAccess)}, +
     		           {AeDebug,        $(NoTitle),	$(MaskAllAccess)} +
		      }
        set DebugPath = $(MstoolsPath)"\windbg -p %ld -e %ld"
    
        set KeyValues = {{Debugger, $(NoTitle), $(!REG_VT_SZ), $(DebugPath)}}
    
        shell "" CreateKey $(!REG_H_LOCAL) $(KeyPath) $(KeyValues)
        CloseRegKey $($R1)
    
        set KeyPath = { +
                       {SOFTWARE,		$(NoTitle),	$(MaskAllAccess)}, +
                       {Microsoft,      $(NoTitle),	$(MaskAllAccess)}, +
                       {"Windows NT",	$(NoTitle),	$(MaskAllAccess)}, +
		               {CurrentVersion,	$(NoTitle),	$(MaskAllAccess)}, +
     		           {AeDebug,        $(NoTitle),	$(MaskAllAccess)} +
		      }
    
        set KeyValues = {{Auto, $(NoTitle), $(!REG_VT_SZ), 0}}
    
        shell "" CreateKey $(!REG_H_LOCAL) $(KeyPath) $(KeyValues)
        CloseRegKey $($R1)
    endif
    
    
    ; Create Installed Dirs Key

    set KeyPath = { +
                    {SOFTWARE,      $(NoTitle), $(MaskAllAccess)}, +
                    {Microsoft,     $(NoTitle), $(MaskAllAccess)}, +
                    {Win32SDK,      $(NoTitle), $(MaskAllAccess)}, +
                    {Directories,   $(NoTitle), $(MaskAllAccess)} +
                  }

    set KeyValues = {{"Install Dir", $(NoTitle), $(!REG_VT_SZ), $(MstoolsRoot)}}

    shell "" CreateKey $(!REG_H_CUSER) $(KeyPath) $(KeyValues)
    CloseRegKey $($R1)


setupdone=+
    set Exit_Code = $(!SETUP_ERROR_SUCCESS)
    read-syms SetupDoneDlg$(STF_UI_LANGUAGE)
    EndWait
    ui start "Done" $(SDKLIBHANDLE)
    ifstr(i) $(DLGEVENT) == "CONTINUE"
        ui pop 1
        goto end
    else
        goto end
    endif

setupnotdone=+
    read-syms SetupNotDoneDlg$(STF_UI_LANGUAGE)
    EndWait
    ui start "NotDone" $(SDKLIBHANDLE)
    ifstr(i) $(DLGEVENT) == "CONTINUE"
        ui pop 1
        goto end
    else
        goto end
    endif

end = +
    install   FreeSetupLibrary
    exit



