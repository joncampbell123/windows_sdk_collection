AFX_IDI_STD_FRAME MENU
BEGIN
    POPUP   "&Session"
    BEGIN
        MENUITEM        "&MAPILogon",               IDM_LOGON,          GRAYED
        MENUITEM        "Session Logof&f",          IDM_LOGOFF,         GRAYED
        MENUITEM        SEPARATOR 
        MENUITEM        "&EnumAdrTypes",            IDM_ENUMADRTYPES,   GRAYED
        MENUITEM        "&QueryIdentity",           IDM_QUERYIDENTITY,  GRAYED
        MENUITEM        "QueryDefaultMsg&Options",  IDM_QUERYDEFMSGOPTS,GRAYED
        MENUITEM        "&GetMsgStoresTable",       IDM_GETSTORESTBL,   GRAYED
        MENUITEM        "Get&LastError",            IDM_SESGETLASTERROR,GRAYED
        MENUITEM        SEPARATOR 
        MENUITEM        "E&xit\tAlt+F4",            IDM_EXIT,           GRAYED
    END

    POPUP   "&MDB"
    BEGIN
        MENUITEM        "&OpenMessageStore",        IDM_OPENMDB,         GRAYED
        MENUITEM        "Store &Logoff",            IDM_LOGOFFMDB,       GRAYED
        MENUITEM        "MDB &Properties",          IDM_MDBPROPS,        GRAYED        
        MENUITEM        "MDB &Support Mask",        IDM_SUPPORTMASK,     GRAYED        
        MENUITEM        SEPARATOR 
        MENUITEM        "Open &Root Folder",        IDM_OPENROOT,        GRAYED        
        MENUITEM        "Open IPM Subtree",         IDM_IPMSUBTREE,      GRAYED        
        MENUITEM        "Open IPM Outbox",          IDM_IPMOUTBOX,       GRAYED        
        MENUITEM        "Open IPM WasteBasket",     IDM_IPMWASTEBASKET,  GRAYED        
        MENUITEM        "Open IPM Sentmail",        IDM_IPMSENTMAIL,     GRAYED        

        MENUITEM        SEPARATOR 
        MENUITEM        "GetReceive&Folder",        IDM_GETREC,          GRAYED
        MENUITEM        "GetReceiveFolder&Table",   IDM_GETRECFLDTBL,    GRAYED
    END

    POPUP   "S&tatus"
    BEGIN
        MENUITEM        "&Open Status Obj",         IDM_OPENSTATUS,     GRAYED
        MENUITEM        "GetStatus&Table",          IDM_GETSTATUSTBL,   GRAYED    
    END

    POPUP   "&Address Book"
    BEGIN
        MENUITEM        "&Address",                 IDM_ADDRESS,        GRAYED
        MENUITEM        "AB &Properties",           IDM_ABPROPS,        GRAYED        
        MENUITEM        "&CreateOneOff",            IDM_CREATEONEOFF,   GRAYED    
        MENUITEM        "&QueryDefRecipOpt",        IDM_QUERYDEFRECIPOPT,   GRAYED    
        MENUITEM        "Resolve&Name",             IDM_RESOLVENAME,    GRAYED    
    END

    POPUP   "&Notification"
    BEGIN
        MENUITEM        "Notification Viewer"       IDM_REGON,         CHECKED 
        MENUITEM        "&Status Notification",     IDM_REGSTATUS,     GRAYED
        MENUITEM        "S&ession Notification",    IDM_REGSESSION,    GRAYED    
        MENUITEM        "&MDB Notification",        IDM_REGMDB,        GRAYED    
        MENUITEM        "&Table Notification",      IDM_REGTABLE,      GRAYED    
        MENUITEM        "&AddressBook Notification",IDM_REGAB,         GRAYED    
    END


    POPUP   "&Help"
    BEGIN
        MENUITEM        "&About...\tF1",            IDM_ABOUT
    END

END


