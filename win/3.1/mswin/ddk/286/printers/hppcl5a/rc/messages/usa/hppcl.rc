STRINGTABLE LOADONCALL MOVEABLE DISCARDABLE
BEGIN
	/* "None" MUST match Windows' string for "None" to indicate
	 *  NO port, like "PCL / HP LaserJet on None"
	 */
	NULL_PORT	"None"

	/*error messages*/
	ERROR_BASE	"PCL Printing Error"
	ERROR_BASE+1 	"MEMORY LIMIT: Too Many Permanently DownLoaded Fonts"
	ERROR_BASE+2 	"Incorrect version of FINSTALL.DLL"
	ERROR_BASE+3 	"PCL Setup Error"
	ERROR_BASE+4 	"MEMORY LIMIT: Too Many Cartridges"
	WARNING_BASE	"PCL Printing Warning"
	/* Use the following when unable to download font for current page
     */
	WARNING_BASE+1	"ERROR DOWNLOADING: Some fonts will be substituted!"

#if defined(WIN31)
        /*the following is used by DevInstall when a printer port is changed. */
        WARNING_BASE+2  "There are printer fonts installed for the old Port. \
To make these fonts available for this new port, you must select the \042Fonts\042 button from the \
printer's Setup dialog box and copy these fonts to the new port."
#endif
	/* Stuff that we read from the win.ini file
	 */
	WININI_PAPER		"paper"
	WININI_COPIES		"copies"
	WININI_ORIENT		"orient"
	WININI_PRTRESFAC	"prtresfac"
