#define DUPLEX

/* Translation tables
 */
XTBL_USASCII   TRANSTBL LOADONCALL MOVEABLE DISCARDABLE usascii.tbl
XTBL_ROMAN8    TRANSTBL LOADONCALL MOVEABLE DISCARDABLE roman8.tbl
XTBL_GENERIC7  TRANSTBL LOADONCALL MOVEABLE DISCARDABLE generic7.tbl
