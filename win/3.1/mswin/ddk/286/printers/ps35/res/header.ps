%
% set current transform matrix (CTM) to map
% from GDI space to USER space
%
%	GDI space	  	   USER space
%
%	+------>	  	   ^
%	|		  	   |
%	|		  	   |
%	|	    -- CTM ->  	   |
%	|		  	   |
%	V		  	   +------->
%  
%
%  The function of this routine is to translate
%  the default origin (lower left corner of paper)
%  to either the upper left or right corner of the
%  imageable region depending on fMirror and fLandscape.
%
%  The axes of the coordinate must then be rotated and
%  inverted as appropriate.
%
%
%        Portrait Mode                 90 Landscape Mode 
%    -------------------------       -------------------------
%    |^                      |       |                       |
%    ||cyM                   |       |                       |
%    ||                      |       |                       |
%    |v     +-NX->...<-RX-+  |       |      ...............  |
%    |      |             |  |       |      :             :  |
%    |      |             |  |       |      :             :  |
%    |      |NY         RY|  |       |      :             :  |
%    |      |             |  |       |      :             :  |
%    |      v             v  |       |      :             :  |
%    |      :             :  |       |      :             :  |
%    |      :             :  |       |      :             :  |
%    |      :             :  |       |      :             :  |
%    |      :             :  |       |      :             :  |
%    |      :             :  |       |      :             :  |
%    |      : Imageable   :  |       |      :  Imageable  :  |
%    |      :             :  |       |      :             :  |
%    |      :   Region    :  |       |      :    Region   :  |
%    |      :             :  |       |      :             :  |
%    |<---->:             :  |       |<---->^             ^  |
%    | cxM  :             :  |       | cxM  |             |  |
%    |      :             :  |       |      |NX         RX|  |
%    |      :             :  |       |      |             |  |
%    |      :             :  |       |      |             |  |
%    |      :.............:  |       |      +-NY->...<-RY-+  |
%    ^                       |       ^                      ^|
%    |                       |       |                      ||
%    |          Physical     |       |           Physical   ||
%    |DY         Paper       |       |DY          Paper     |cyM
%    |            Size       |       |             Size     ||
%    |                       |       |                      ||
%    | DX                    |       |  DX                  v|
%    +----->------------------       +------>-----------------
%
%
%
%
%        270 Landscape Mode
%    -------------------------
%    |^                      |
%    ||cyM                   |
%    ||                      |
%    |v     +-RY->...<-NY-+  |
%    |      |             |  |
%    |      |             |  |
%    |      |RX         NX|  |
%    |      |             |  |
%    |      v             v  |
%    |      :             :  |
%    |      :             :  |
%    |      :             :  |
%    |      :             :  |
%    |      :             :  |
%    |      : Imageable   :  |
%    |      :             :  |
%    |      :   Region    :  |
%    |      :             :  |
%    |<---->:             :  |
%    | cxM  :             :  |
%    |      :             :  |
%    |      :             :  |
%    |      :             :  |
%    |      :.............:  |
%    ^                       |
%    |                       |
%    |          Physical     |
%    |DY         Paper       |
%    |            Size       |
%    |                       |
%    | DX                    |
%    +----->------------------
%  
%  
%  
%  
%  DX, DY:  Default Postscript cooridinate system
%  
%  NX, NY:  normal  cooridinate system
%  RX, RY:  reflected cooridinate system (selected by fMirror)
%  cxM:     width of lefthand unprintable margin
%  cyM:     width of top unprintable margin for portrait and 270 landscape
%           OR  width of bottom unprintable margin for  90 landscape
%
%  Note:    all dimensions are in units of 1/100 ths of an inch.
%           
%  
%  
% set metrics
%
% fMirror LandscapeOr cxM cyM cxPg cyP iRes SM --

/SM
{	% grab all the parameters

 	/iRes ed		% res in dots/inch
	/cyP ed 		% paper height
	/cxPg ed 		% imageable page width
	/cyM ed 		% see above comments
	/cxM ed 		% image  offset from left of paper

#ifdef FULL_HEADER
	% scale user units to GDI units

 	72 100 div dup
 	scale

	% on the stack now is LandscapeOr
	% 0 == portrait (default)
	% 90, 270 == landscape

        dup

        0 ne    %  if LandscapeOr == 90 or 270
	{
            90 eq
            {
           	% 90 deg landscape mode
                cyM exch    % amount to move up, xpose fMirror

                0 eq     %  if not fMirror
                {   % normal parity
                        cxM  exch  tr  
                        -90  rotate
                        -1 1 scale             % point axis 
                }
                {   % fMirror
                        cxM  cxPg  add  exch  tr  
                        +90  rotate             % point axis 
                } ifelse
            }
            {
                % 270 deg landscape mode
                cyP cyM sub exch    % amount to move up, xpose fMirror

                0 ne     %  if fMirror
                {   % invert coordinates
                        cxM  exch  tr  
                        -90  rotate             % point axis 
                }
                {   % normal parity
                        cxM  cxPg  add  exch  tr  
                        -90  rotate             % point axis 
                        1 -1 scale              % flip y axis down
                } ifelse
            } ifelse
        }
	{	% portrait mode
                pop   % dispose of duplicate LandscapeOr
                cyP cyM sub exch    % amount to move up, xpose fMirror

                0 ne      %  if fMirror
                {   % invert coordinates
                        cxM  cxPg  add  exch  tr  % move to upper right
                        180  rotate             % point axis 
                }
                {   % normal parity
                        cxM  exch  tr           % move to upper left
                        1 -1 scale              % flip y axis down
                } ifelse
        }  ifelse


        % set scale to iRes units per inch

        100 iRes div dup scale

	% align the PS coor sys to avoid rounding problems

	0 0 transform .25 add round .25 sub exch
		      .25 add round .25 sub exch
	itransform translate
#else

	% short header version.  here we just move the origin of the
	% page.  we ignore unprintable margins. we don't scale.

	% on the stack now is fLandscape
	% 1 == landscape

 	0 ne
	{	% landscape mode

		0 cyP 72 mul 100 div tr 	% move to upper left

		-90 rotate			% swing x axis down
	} if
        pop     % discard fMirror  parameter
#endif
} bd


#ifdef FULL_HEADER

%
% SetJustify(TBreakExtra, BreakCount)
%
% sets global justify parameters for the next StrBlt. dxBreak fBE
%
/SJ
{
	1 index 0 eq	% get TBreakExtra
	{
		pop pop		% clear the stack
		/fBE false def	% turn off if 0 BreakCount
	} {
		1 index		% get the Break value (# pixels to adjust)
		/Break ed	% save it for use in SB
		div		% dxBreak = TBreakExtra / BreakCount
		/dxBreak ed	% insert this much for each
		/fBE true def	% tell StrBlt to use these
	} ifelse
} bd


