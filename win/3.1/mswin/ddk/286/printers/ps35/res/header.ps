/PP
{
 	{ rlt } repeat
} bd



%
% OpaqueBox
%
% dx dy x y r g b fText OB --
%
% used by ExtTextOut and BitBlt
%
/OB
{
	gs
        0 ne {
                7 3 roll                % r g b dx dy x y
                /y ed /x ed             % r g b dx dy
                x y translate           % move points to origin
                ULesc rotate            % rotate them
                x neg y neg translate   % move them back where they belong
                x y 7 -3 roll           % dx dy x y r g b
        } if

	sc
	B
	fill
	gr
} bd


