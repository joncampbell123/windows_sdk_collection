            _IDATA CLASS 'ICODE' DISCARDABLE
            _TEXT  CLASS 'PCODE' NONDISCARDABLE
            _DATA  CLASS 'PCODE' NONDISCARDABLE

EXPORTS     VSBD_DDB    @1
