        CONTROL "",COLOR_HUE, "ScrollBar",
                WS_VISIBLE | SBS_HORZ | SBS_TOPALIGN | WS_TABSTOP | WS_GROUP,
                SCROLLS_X+28,SCROLLS_Y+10,92,12

