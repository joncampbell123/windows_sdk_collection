        CONTROL "",COLOR_BRIGHT, "ScrollBar",
                WS_VISIBLE | SBS_HORZ | SBS_TOPALIGN | WS_TABSTOP | WS_GROUP,
                SCROLLS_X+28,SCROLLS_Y+34,92,12

        LTEXT   "&Color", FOO, SCROLLS_X,SCROLLS_Y+58,24,8, WS_GROUP
        CONTROL "",COLOR_COLOR, "ScrollBar",
                WS_VISIBLE | SBS_HORZ | SBS_TOPALIGN | WS_TABSTOP | WS_GROUP,
