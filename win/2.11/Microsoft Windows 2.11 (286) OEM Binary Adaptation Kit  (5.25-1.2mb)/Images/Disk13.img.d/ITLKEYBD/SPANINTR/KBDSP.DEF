
LIBRARY KEYBOARD

DESCRIPTION 'KEYBOARD Spanish international (IBM - Olivetti - Ericsson ...):'

STUB 'WINSTUB.EXE'

CODE PRELOAD FIXED
DATA PRELOAD FIXED SINGLE

SEGMENTS
        _INIT PRELOAD FIXED DISCARDABLE
        _NORES LOADONCALL DISCARDABLE
        _NORES1 LOADONCALL DISCARDABLE

EXPORTS
    Inquire     @1
    Enable      @2
    Disable     @3
    ToAscii     @4
    AnsiToOem   @5
    OemToAnsi   @6
    SetSpeed    @7

; Special call for OS/2 Compatibility box
    ScreenSwitchEnable  @100

; Special call for WIN386 WINOLDAP
    OEMKeyScan  @128

; Special ASCII to virtual keycode routine

    VkKeyScan	@129

IMPORTS
    KbdRst =    kernel.123
