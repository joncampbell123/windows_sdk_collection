[SetupData]
#CD-ROM:
SetupType = 0
DefaultLayout = 00000409
LoadIdentifier = "Windows NT Version 3.1"
MinimumMemory = 16777216
ProductType = 0
[debug]
# if CopySource is specified, use the given name as the copy source
#CopySource = scsi(0)disk(3)rdisk(0)partition(1)
# if DiagnosticOutputPort is specified, print error msgs to the given
# com port (0 = com1, etc).
#DiagnosticOutputPort = 0
# if DontCopy is specified and is non-0, Setup will not actually copy any
# files but will function otherwise normally.
#DontCopy = 1
# if DebugBoot is 1, then the system will not be booted /nodebug
DebugBoot = 0
# If specified and non-0, the TextModeOnly flag causes setup to
# pause at the end of text mode, allowing the user to exit or continue.
#TextModeOnly = 1
#
# The values below configure the test. Defaults are drive 1, 1st partition
# No 3rd party driver additions, no repartition/format.
#
TestDisk = 1
TestPart = 0
Custom = 0
NoKill = 1




