call doinit

@rem
@rem Format, write, read ...
@rem 

call doformat %label% %fileSystem% %driveLetter1% %logfile%

call dosetmap %driveLetter1%:\foo.bin %_fill_percent% %logfile%

call dodir %driveLetter1% %logfile%  
 
call doendmd %driveLetter1%:\foo.bin %logfile% 

call dochkdsk %driveLetter1% %fileSystem% %logfile%

@rem
@rem NOTENOTE: could do lot of stuff here
@rem

@rem
@rem Delete the partition and move to next variation
@rem

echo.                                                        >> %logfile%
echo TRACE: End of variation - about to delete the partition >> %logfile%
echo.                                                        >> %logfile%

copy do.0 donext
mtrun delete.pcd /h
exit
