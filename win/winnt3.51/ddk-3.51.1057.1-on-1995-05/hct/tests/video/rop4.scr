
;
; Section: Rop4
;
; sets various global flags
;
; fDbgFlag:
;    1:   verbose
;    2:   Print Rop number
;    4:   Show bitmaps
;    8:   Break on errors
;
; autoexit: if =1 then exit rop4 when done with script
; maximize: if =1 then maximize window before doing test
;
[Rop4]
fDbgFlag=4
MaxErrors=20
autoexit=1
maximize=1

;
; Section: Sizes
;
; Sets sizes to test
;
[Sizes]
sizes=12-12

;
; Section: Rops
;
; for testing rops using Bitblt
;
; values: donttest and onlytest are lists of rops to test
;  onlytest overrides donttest
;
[Rops]
onlytest=all

;
; MaskBlt sections 
;
; forerop and backrop sections are for maskblt testing.
;
; forerop contains list of rops to not test or only test
;
; onlytest overrides donttest 
;
; comma delimited with dashes used for ranges
;
; 0-255    sets all 
; none     sets none
; all      sets all
[forerop]
onlytest=all

[backrop]
onlytest=all
