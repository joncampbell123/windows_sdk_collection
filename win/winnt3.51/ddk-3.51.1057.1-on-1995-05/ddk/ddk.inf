    else-ifstr(i) $(DLGEVENT) == "CUSTOMISE"

        set DoBuildEnv  = *($(CheckItemsOut), 1)
        set DoHelp      = *($(CheckItemsOut), 2)
        set DoSamples   = *($(CheckItemsOut), 3)

        ui pop 1
