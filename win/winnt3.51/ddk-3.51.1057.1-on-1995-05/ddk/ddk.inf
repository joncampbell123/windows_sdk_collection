        ForListDo $(SamplesList)
            ifstr(i) $(Do$($)) == $(Chosen)
                set-add SamplesSize = $(SamplesSize) $($($)Size)
            endif
        EndForListDo
    endif


    set-sub FreeForOptional = $(DestVolumeFree) $(MinToolsSize)
