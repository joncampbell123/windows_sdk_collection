	ui start "Options" $(DDKLIBHANDLE)
	ifstr(i) $(DLGEVENT) == "DLGBUTTON0" ;button titled OK, IDC_BTN0

            set DoGeneral   = *($(CheckItemsOut), 1)
            set DoNetwork   = *($(CheckItemsOut), 2)
            set DoVideo     = *($(CheckItemsOut), 3)
            set DoSCSI      = *($(CheckItemsOut), 4)
            set DoInput     = *($(CheckItemsOut), 5)
            set DoPrinter   = *($(CheckItemsOut), 6)
            set DoMM        = *($(CheckItemsOut), 7)
            set DoComm      = *($(CheckItemsOut), 8)
            set DoOther     = *($(CheckItemsOut), 9)
	    ui pop 1
	    goto ddk
	else-ifstr(i) $(DLGEVENT) == "EXIT"
            shell "" QueryUserQuit
            ifstr(i) $($R1) == "OK"
                ui pop 1
                goto setupnotdone
            else
                goto ddksamples
            endif
        endif



    else-ifstr(i) $(DLGEVENT) == "EXIT"
        shell "" QueryUserQuit
        ifstr(i) $($R1) == "OK"
            ui pop 1
            goto setupnotdone
        else
            goto ddk
        endif
    else
