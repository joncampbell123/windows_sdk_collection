[Install-DDK-Groups]

    ifstr(i) $(PRIVILEGES) == "SYSTEM"
	set Common = "Common"
    else
        set Common = ""
    endif

    set ToolsGroup = "Windows NT DDK"
    Create$(Common)ProgManGroup $(ToolsGroup) ""
    Show$(Common)ProgManGroup   $(ToolsGroup), 1


    ifstr(i) $(!DoBuildEnv) == $(!Chosen)
        Remove$(Common)ProgmanItem  $(ToolsGroup) , "Free Build Environment"
        Remove$(Common)ProgmanItem  $(ToolsGroup) , "Checked Build Environment"
        Create$(Common)ProgmanItem  $(ToolsGroup) , "Free Build Environment" "cmd /k "$(DDKBin)"\setenv.bat "$(BASEDIR)" free" $(DDKBin)"\freebld.ico" 0
        Create$(Common)ProgmanItem  $(ToolsGroup) , "Checked Build Environment" "cmd /k "$(DDKBin)"\setenv.bat "$(BASEDIR)" checked" $(DDKBin)"\chkbld.ico" 0
    endif

    ifstr(i) $(!DoHelp) == $(!Chosen)
        Remove$(Common)ProgmanItem  $(ToolsGroup) , "Getting Started"
        Remove$(Common)ProgmanItem  $(ToolsGroup) , "DDK Help"
        Create$(Common)ProgmanItem  $(ToolsGroup) , "Getting Started" "winhlp32 "$(DDKHlp)"\ddkstart.hlp" "" 0
        Create$(Common)ProgmanItem  $(ToolsGroup) , "DDK Help"  $(DDKHlp)"\msin32 NTDDK.MVB" "" 0
    endif

    Show$(Common)ProgManGroup   $(ToolsGroup), 6
    exit

