HKR,NDI\params\Burst,ParamDesc,,"DMA Burst Size (2-14)"
HKR,NDI\params\Burst,flag,1,30,00,00,00
HKR,NDI\params\Burst,type,,edit
HKR,NDI\params\Burst,LimitText,,10
HKR,NDI\params\Burst,UpperCase,,1
HKR,NDI\params\Burst,default,,"DEFAULT"
