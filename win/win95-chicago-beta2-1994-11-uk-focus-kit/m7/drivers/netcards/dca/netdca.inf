[version]
LayoutFile=layout.inf
signature="$CHICAGO$"
Class=Net
provider=%V_MS%

[Manufacturer]
%V_DCA%=DCA

[DCA]
%*pnpDCAI.DeviceDesc%=*pnpDCAI.ndi, *pnpDCAI    ;DCA IRMATrac Convertible ISA
%mca_5C1D.DeviceDesc%=*mca_5C1D.ndi, mca_5C1D    ;DCA IRMATrac Phase II MCA
%mca_5C1C.DeviceDesc%=*mca_5C1C.ndi, mca_5C1C    ;DCA IRMATrac Phase I MCA

;****************************************************************************
;*pnpDCAI       ;DCA IRMATrac Convertible ISA
;****************************************************************************
[*pnpDCAI.ndi]
AddReg=*pnpDCAI.ndi.reg,IRMATR.ndi.reg, ISA.ndi.resc
LogConfig=*pnpDCAI.LogConfig

[*pnpDCAI.ndi.reg]
HKR,Ndi,DeviceID,,"*PNPDCAI"

[*pnpDCAI.LogConfig]
ConfigPriority=HARDRECONFIG
IOConfig=A00-A1F(3FF::),A20-A3F(3FF::),A40-A5F(3FF::),A60-A7F(3FF::),1A00-1A1F(3FF::),1A20-1A3F(3FF::),1A40-1A5F(3FF::),1A60-1A7F(3FF::)
IRQConfig=2,3,4,5,6,7,10,11
DMAConfig=3,5,6,7

;****************************************************************************
;*mca_5C1D       ;DCA IRMATrac Convertible MCA
;****************************************************************************
[*mca_5C1D.ndi]
AddReg=*mca_5C1D.ndi.reg,IRMATR.ndi.reg,IRMATR.General.Params,slotnumber

[*mca_5C1D.ndi.reg]
HKR,Ndi,DeviceID,,"*MCA_5C1D"

[IRMATR.General.params]
;Earlyrelease
HKR,Ndi\params\earlyrelease,ParamDesc,,"Early Release"
HKR,Ndi\params\earlyrelease,default,,Off
HKR,Ndi\params\earlyrelease,type,,enum
HKR,Ndi\params\earlyrelease\enum,Off,,"Off"
HKR,Ndi\params\earlyrelease\enum,On,,"On"
HKR,NDI\params\earlyrelease,flag,1,30,00,00,00
