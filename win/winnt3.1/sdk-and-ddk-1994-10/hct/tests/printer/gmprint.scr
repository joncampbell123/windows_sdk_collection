;------------------------------------------------------------------------------
; GUIMAN PRINTER TEST SUITE
;   This script tests the output to the printer.  The metafiles are loaded
;   and printed.
;
;   [Header]
;     TestType    : STRESS  FUNCTIONALITY
;     Flags       : REGION DDE PRINT CLIPCOPY CLIPPASTE CONVERT CONVERTSAVE
;                   XFORM SCALE AUTOEXIT COMPATSURFACE
;     Verify      : OFF  AUTO  FILE  GENVER
;     LogFile     : <name of logfile>
;     LogStyle    : <ntlog styles>
;     Timer       : <delay in milliseconds>
;     Iteration   : <number of cycles through script>
;
;
;   [Tesst(n)]
;     Description : <string representing the testname>
;     tstdir      : <directory of test files (.emf, .mf, .wmf)>
;     verdir      : <directory of verify files (.ver)>
;     Var(n)      : <testfile> <verification file (optional)>
;
;------------------------------------------------------------------------------

[header]
TestType=FUNCTIONAL
Flags=PRINT SCALE AUTOEXIT
Verify=OFF
LogFile=gmprint.log
LogStyle=LOGALL REFRESH
Timer=4000
Iteration=1



[test0]
description=Brush Alignment Test
tstdir=.\
verdir=.\
var0=brush00.emf brush00.ver
var1=brush01.emf brush01.ver
var2=brush04.emf brush04.ver
var3=brush05.emf brush05.ver
var4=brush09.emf brush09.ver
var5=brush12.emf brush12.ver
var6=brush13.emf brush13.ver
var7=brush16.emf brush16.ver
var8=brush19.emf brush19.ver


